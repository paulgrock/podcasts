  "private": true,
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "concurrently": "^3.5.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.4",
    "nodemon": "^1.17.3",
    "prettier": "^1.12.0",
    "prettier-eslint": "^8.8.1",
    "webpack": "^3.11.0"
  },
  "dependencies": {
    "apollo-boost": "^0.1.4",
    "apollo-client": "^2.2.8",
    "body-parser": "^1.18.2",
    "date-fns": "^1.29.0",
    "dotenv": "^5.0.1",
    "esm": "^3.0.17",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "express-jwt": "^5.3.1",
    "express-session": "^1.15.6",
    "graphql": "^0.13.2",
    "mongoose": "^5.0.14",
    "react": "^16.3.1",
    "react-apollo": "^2.1.3",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "redux": "^3.7.2",
    "slugify": "^1.2.9"
  },
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "client": "react-scripts start",
    "server": "nodemon --inspect=9292 index",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "lint-staged"
  },
  "proxy": {
    "/api": {
      "target": "http://localhost:3001/"
    }
  },
  "@std": {
    "esm": {
      "mode": "auto",
      "esm": "js"
    }
  },
  "prettier": {
    "singleQuote": true,
    "useTabs": true
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
